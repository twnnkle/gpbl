---
import Container from '@layouts/Container.astro';

import Star from '@components/common/Star.astro';
import SideMenu from '@components/pages/myPage/myPage2/SideMenu.astro';
---

<body>
  <Container>
    <main>
      <div class="my-page-page">
        <div class="row justify-content-between">
          <div class="col-md-3">
            <SideMenu />
          </div>

          <div class="col-md-9">
            <div class="my-page-page__content">
              <div class="my-page-page__iframe">
                <iframe
                  src=""
                  width="100%"
                  height="650px"></iframe>
              </div>
              <Star />
            </div>
          </div>
        </div>
      </div>
    </main>
  </Container>
</body>

<style lang="scss">
  body {
    background-color: #f3f5fb;
  }

  .my-page-page {
    margin: 50px 0;

    &__content {
      display: flex;
      gap: 30px;
    }

    &__iframe {
      flex: 1;
      margin-top: 30px;
    }
  }
</style>
